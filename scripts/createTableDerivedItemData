CREATE TABLE PUBLIC.DERIVEDITEMDATA (
	ID BIGINT NOT NULL,
	ITEMID INTEGER NOT NULL,
	REGIONID INTEGER NOT NULL,
	OWNERREALMID INTEGER NOT NULL,
	LASTMODIFIED TIMESTAMP NOT NULL,
	NUMBEROFAUCTIONS INTEGER,
	PRICEPER1 INTEGER,
	PRICEPER10 INTEGER,
	PRICEPER100 INTEGER,
	PRICEPER1000 INTEGER,
	CONSTRAINT DERIVEDITEMDATA_PL PRIMARY KEY (ID),
	CONSTRAINT DERIVEDITEMDATA_FK_REGION FOREIGN KEY (REGIONID) REFERENCES PUBLIC.REGION(REGIONID) ON DELETE RESTRICT ON UPDATE RESTRICT,
	CONSTRAINT DERIVEDITEMDATA_FK_REALM FOREIGN KEY (OWNERREALMID) REFERENCES PUBLIC.REALM(REALMID) ON DELETE RESTRICT ON UPDATE RESTRICT,
	CONSTRAINT DERIVEDITEMDATA_FK_ITEM FOREIGN KEY (ITEMID) REFERENCES PUBLIC.ITEM(ID) ON DELETE RESTRICT ON UPDATE RESTRICT
);

CREATE INDEX PRIMARY_KEY_A ON PUBLIC.DERIVEDITEMDATA (ID);